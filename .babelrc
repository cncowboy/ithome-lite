{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-runtime",
    ["import", {
      "libraryName": "xanui",
      "libraryDirectory": "es",
      "customName": "(transformedMethodName) => { var result = this.libraryName; if (this.libraryDirectory) { result += '/'+ this.libraryDirectory; } if (transformedMethodName) { result += '/'+ transformedMethodName.replace('xan-', ''); } if (this.fileName) { result += '/'+ this.fileName; } return result; }",
      "style": true
    }]
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
